{
  "name": "drinks",
  "version": "0.0.0-semantically-released",
  "description": "Craft Cocktail Gallery",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/drinks.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/drinks/issues"
  },
  "scripts": {
    "clean": "del-cli .cache ./app/icons ./build ./public/build ./public/sw.js",
    "build": "run-s build:*",
    "build:icons": "rmx svg-sprite ./other/svg-icons ./app/icons --sprite=icons-sprite.svg --template=./other/svg-icons/template.tsx",
    "build:remix": "remix build",
    "build:worker": "remix-pwa build",
    "deploy": "flyctl deploy --remote-only",
    "dev": "run-s clean build:icons && run-p dev:*",
    "dev:remix": "remix dev --manual --command \"tsx watch --clear-screen=false --inspect --require dotenv/config --watch-path ./server ./server/index.ts\"",
    "dev:worker": "remix-pwa dev",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev",
    "start": "cross-env NODE_ENV=production tsx ./server/index.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@epic-web/remember": "1.0.2",
    "@fontsource/source-sans-3": "5.0.18",
    "@prisma/client": "5.8.1",
    "@remix-pwa/cache": "2.0.12",
    "@remix-pwa/strategy": "2.1.9",
    "@remix-pwa/sw": "2.1.12",
    "@remix-run/css-bundle": "2.5.1",
    "@remix-run/express": "2.5.1",
    "@remix-run/node": "2.5.1",
    "@remix-run/react": "2.5.1",
    "@remix-run/serve": "2.5.1",
    "algoliasearch": "4.22.1",
    "clsx": "2.1.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "isbot": "4.4.0",
    "litefs-js": "1.1.2",
    "lodash-es": "4.17.21",
    "marked": "11.2.0",
    "morgan": "1.10.0",
    "p-throttle": "6.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix": "2.5.1",
    "source-map-support": "0.5.21",
    "tsx": "4.7.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@remix-pwa/dev": "2.0.31",
    "@remix-pwa/worker-runtime": "2.0.8",
    "@remix-run/dev": "2.5.1",
    "@sly-cli/sly": "1.8.0",
    "@types/compression": "1.7.5",
    "@types/express": "4.17.21",
    "@types/lodash-es": "4.17.12",
    "@types/marked": "5.0.2",
    "@types/morgan": "1.9.9",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/source-map-support": "0.5.10",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "@wkovacs64/prettier-config": "3.0.3",
    "autoprefixer": "10.4.17",
    "chokidar": "3.5.3",
    "del-cli": "5.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "10.3.10",
    "node-html-parser": "6.1.12",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.11",
    "prisma": "5.8.1",
    "remix-pwa": "3.0.19",
    "rmx-cli": "0.4.15",
    "semantic-release": "23.0.0",
    "shelljs": "0.8.5",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20"
  }
}
